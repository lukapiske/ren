   <tr>
      <td><b><%= I18n.t('kpi.cmp') %></b></td>
      <td><%=h number_with_precision(@cmp , :precision => 2) %></td>
      <td><%=h number_with_precision(@cmpc , :precision => 2) %></td>
      <td><%=h number_with_precision(outlet.cvc( @cmp, @cmpc ), :precision => 2)  %></td>
      <td><%=h number_with_precision(outlet.cvcly( @cmp , @cmply), :precision => 2)  %></td>
   </tr>
